@prefix coptic: <http://www.semanticweb.org/sjhuskey/ontologies/2025/7/coptic-metadata-viewer/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix frbr: <http://purl.org/vocab/frbr/core#> .
@prefix lawd: <http://lawd.info/ontology/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lexinfo: <https://lexinfo.net/ontology/3.0/lexinfo#> .
@prefix schema1: <https://schema.org/> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .

<http://www.semanticweb.org/sjhuskey/ontologies/2025/7/coptic-metadata-viewer/> rdf:type owl:Ontology ;
                                                                                rdfs:comment "A custom ontology for data downloaded from the PAThs API."@en .

#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:ObjectProperty ;
                rdfs:domain frbr:Work ;
                rdfs:range coptic:Author ;
                rdfs:comment "A work has an author as a creator."@en .


###  http://purl.org/dc/terms/hasPart
dcterms:hasPart rdf:type owl:ObjectProperty ;
                owl:inverseOf dcterms:isPartOf ;
                rdf:type owl:TransitiveProperty ;
                rdfs:domain dcmitype:Collection ,
                            coptic:Manuscript ;
                rdfs:comment "A collection or a manuscript has parts. The inverse is dcterms:isPartOf"@en .


###  http://purl.org/dc/terms/isPartOf
dcterms:isPartOf rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty .


###  http://purl.org/dc/terms/isReferencedBy
dcterms:isReferencedBy rdf:type owl:ObjectProperty ;
                       owl:inverseOf dcterms:references ;
                       rdf:type owl:TransitiveProperty ;
                       rdfs:comment "Inverse of dcterms:references."@en .


###  http://purl.org/dc/terms/references
dcterms:references rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:comment "A title references a work."@en .


###  https://schema.org/birthPlace
schema1:birthPlace rdf:type owl:ObjectProperty ;
                   rdfs:comment "The birthplace of a person"@en .


###  https://schema.org/gender
schema1:gender rdf:type owl:ObjectProperty ;
               rdfs:comment "A person has gender"@en .


###  https://schema.org/roleName
schema1:roleName rdf:type owl:ObjectProperty ;
                 rdfs:comment "The occupation or title of a person."@en .


#################################################################
#    Data properties
#################################################################

###  http://lawd.info/ontology/primaryForm
lawd:primaryForm rdf:type owl:DatatypeProperty ;
                 rdfs:range rdfs:Literal ;
                 rdfs:comment "The preferred name of a place."@en .


###  http://purl.org/dc/terms/bibliographicCitation
dcterms:bibliographicCitation rdf:type owl:DatatypeProperty ;
                              rdfs:range xsd:string ;
                              rdfs:comment "A manuscript has citations in modern publications."@en .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:long ,
                               xsd:string ;
                    rdfs:comment "A catch-all field for describing an item in the database."@en .


###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Any identifier by which an item is known."@en .


###  http://purl.org/dc/terms/medium
dcterms:medium rdf:type owl:DatatypeProperty ;
               rdfs:range rdfs:Literal ;
               rdfs:comment "The material of a manuscript."@en .


###  http://purl.org/dc/terms/spatial
dcterms:spatial rdf:type owl:DatatypeProperty ;
                rdfs:range rdfs:Literal .


###  http://purl.org/dc/terms/temporal
dcterms:temporal rdf:type owl:DatatypeProperty ;
                 rdfs:range rdfs:Literal .


###  http://purl.org/dc/terms/type
dcterms:type rdf:type owl:DatatypeProperty ;
             rdfs:range rdfs:Literal .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:anyURI ;
           rdfs:comment "Identifies the same thing as cataloged in another database."@en .


###  http://www.w3.org/2004/02/skos/core#exactMatch
<http://www.w3.org/2004/02/skos/core#exactMatch> rdf:type owl:DatatypeProperty ;
                                                 rdfs:range xsd:anyURI .


###  http://www.w3.org/2006/time#hasBeginning
time:hasBeginning rdf:type owl:DatatypeProperty ;
                  rdfs:range rdfs:Literal .


###  http://www.w3.org/2006/time#hasEnd
time:hasEnd rdf:type owl:DatatypeProperty ;
            rdfs:range rdfs:Literal .


###  http://xmlns.com/foaf/0.1/name
<http://xmlns.com/foaf/0.1/name> rdf:type owl:DatatypeProperty ;
                                 rdfs:range rdfs:Literal ;
                                 rdfs:comment "The name of a person."@en .


###  https://lexinfo.net/ontology/3.0/lexinfo#translation
lexinfo:translation rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:long .


###  https://lexinfo.net/ontology/3.0/lexinfo#transliteration
lexinfo:transliteration rdf:type owl:DatatypeProperty ;
                        rdfs:range rdfs:Literal .


###  https://schema.org/title
schema1:title rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "An honorific or a position."@en .


#################################################################
#    Classes
#################################################################

###  http://lawd.info/ontology/Place
lawd:Place rdf:type owl:Class ;
           rdfs:comment "A geographical location relevant to Coptic texts."@en ;
           rdfs:label "Place"@en .


###  http://purl.org/dc/dcmitype/Collection
dcmitype:Collection rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty dcterms:hasPart ;
                                      owl:someValuesFrom coptic:Manuscript
                                    ] ;
                    rdfs:comment "A library, museum, private collection, etc., that houses one or more manuscripts or manuscript fragments."@en ;
                    rdfs:label "Collection"@en .


###  http://purl.org/dc/terms/PhysicalResource
dcterms:PhysicalResource rdf:type owl:Class ;
                         owl:equivalentClass coptic:Manuscript .


###  http://purl.org/vocab/frbr/core#Work
frbr:Work rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty dcterms:creator ;
                            owl:someValuesFrom coptic:Author
                          ] ;
          rdfs:comment "The concept of a work by an author."@en ;
          rdfs:label "Work"@en .


###  http://www.semanticweb.org/sjhuskey/ontologies/2025/7/coptic-metadata-viewer/Author
coptic:Author rdf:type owl:Class ;
    rdfs:subClassOf dcterms:Agent ;
    rdfs:comment "An author of a work."@en ;
    rdfs:label "Author"@en .


###  http://www.semanticweb.org/sjhuskey/ontologies/2025/7/coptic-metadata-viewer/Colophon
coptic:Colophon rdf:type owl:Class ;
    rdfs:subClassOf dcmitype:Text ,
        [ a owl:Restriction ;
          owl:onProperty dcterms:type ;
          owl:hasValue "colophon"
        ] ;
    rdfs:comment "A text node representing a colophon in a manuscript." .


###  http://www.semanticweb.org/sjhuskey/ontologies/2025/7/coptic-metadata-viewer/Manuscript
coptic:Manuscript rdf:type owl:Class ;
    rdfs:subClassOf dcterms:PhysicalResource ,
        [ a owl:Restriction ;
          owl:onProperty dcterms:isPartOf ;
          owl:someValuesFrom dcmitype:Collection
        ] ;
    rdfs:comment "A manuscript containing Coptic and/or Graeco-Coptic text."@en ;
    rdfs:label "Manuscript"@en .


###  http://www.semanticweb.org/sjhuskey/ontologies/2025/7/coptic-metadata-viewer/Title
coptic:Title rdf:type owl:Class ;
    rdfs:subClassOf dcmitype:Text ,
        [ a owl:Restriction ;
          owl:onProperty dcterms:type ;
          owl:hasValue "title"
        ] ;
    rdfs:comment "A text node representing the title of a manuscript." .

###  http://xmlns.com/foaf/0.1/Person
<http://xmlns.com/foaf/0.1/Person> rdf:type owl:Class ;
                                   rdfs:comment "A person mentioned in a colophon."@en ;
                                   rdfs:label "Person"@en .


